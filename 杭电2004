#include <stdio.h>
int main (){
	int a;
	while (scanf("%d",&a)!=EOF){
		if (a<0||a>100) {
			printf ("Score is error!\n");
			
		}
		else if (a>=0&&a<=59){
			printf("E\n");
		}
		else if (a>=60&&a<=69){
			printf ("D\n");
		}
		else if (a>=70&&a<=79){
			printf ("C\n");
		}
		else if (a>=80&&a<=89){
			printf ("B\n");
		}
		else if (a>=90&&a<=100){
			printf ("A\n");
		}
	}
}
//很傻的if else 在想有没有其他方法
